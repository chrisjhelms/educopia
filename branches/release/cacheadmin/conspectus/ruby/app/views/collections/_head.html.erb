<%= render :partial => "/collections/headline" %>

<table class="show_record_table">
<tr>
  <td >Archive:  </td>
  <td> <%= link_to@collection.archive.title, @collection.archive %>   </td>
</tr>

<tr>
  <td>Plugin:</td>
  <td> <%= plugin_display_name_link(@collection.plugin) %>
   by 
  <%= link_to @collection.content_provider.name, @collection.content_provider %>
  </td>
</tr>

<tr>
  <td>Base Url:</td>
  <td> <%= link_to_ext @collection.base_url,  @collection.base_url %>  </td> 
</tr>

<tr>
  <td>Identifier:</td>
  <td> <%= @collection.id %>  </td> 
</tr>




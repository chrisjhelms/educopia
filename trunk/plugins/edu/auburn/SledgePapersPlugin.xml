  <entry>
    <string>plugin_notes</string>
    <string>This plugin is for harvesting input files for the Eugene B. Sledge Papers collection at Auburn University. There are no parameters. The baseURL is http://digdata1.lib.auburn.edu/numphotos/Sledge Papers/ and the manifest page is manifest.html in this directory. And Bill was here.
</string>
  </entry>
  <entry>
    <string>au_crawlrules</string>
    <list>
      <string>4,&quot;^%s&quot;, base_url</string>
      <string>1,&quot;%s[^/]+.*/&quot;, base_url</string>
      <string>1,&quot;[^/]+.*&quot;</string>
    </list>
  </entry>
  <entry>
    <string>au_start_url</string>
    <string>&quot;%smanifest.html&quot;, base_url</string>
  </entry>
  <entry>
    <string>plugin_config_props</string>
    <list>
      <org.lockss.daemon.ConfigParamDescr>
        <key>base_url</key>
        <displayName>Base URL</displayName>
        <description>Usually of the form http://&lt;journal-name&gt;.com/</description>
        <type>3</type>
        <size>40</size>
        <definitional>true</definitional>
        <defaultOnly>false</defaultOnly>
      </org.lockss.daemon.ConfigParamDescr>
    </list>

#!/bin/csh  -f
source ~/.metaarchive.rc 
set PWD = dingDong
set USER = mmevenk
set DB = ma_cachemanager_prod;  
foreach f ($MACPS) 
  set pre = `echo $f | sed 's/\./|/g'`;
  echo "> $pre"; 
  set stmt =  "SELECT cache_archival_units.key_string, cache_archival_units.disk_usage, cache_archival_units.size, caches.name FROM cache_archival_units \
      JOIN caches ON cache_archival_units.cache_id = caches.id WHERE cache_archival_units.key_string LIKE '$pre%'; "; 
  echo $stmt | mysql --user $USER --password=$PWD $DB    > "$MASTATUS/$f-CurrentAuSizes.csv"
end 
